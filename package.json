{
  "name": "adt-studio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "electron-dist/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "pipeline": "tsx lib/cli/pipeline.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run metadata-integration text-classification-integration",
    "test:recache": "RECACHE=1 vitest run metadata-integration text-classification-integration",
    "test:coverage": "vitest run --coverage",
    "electron:build-ts": "tsc -p electron/tsconfig.json && tsc -p electron/tsconfig.preload.json",
    "electron:copy-assets": "node scripts/copy-electron-assets.mjs",
    "electron:dev": "pnpm build && pnpm electron:copy-assets && pnpm electron:build-ts && electron .",
    "electron:dist": "pnpm build && pnpm electron:build-ts && electron-builder --config electron-builder.yml",
    "electron:dist-mac": "pnpm build && pnpm electron:build-ts && electron-builder --config electron-builder.yml --mac",
    "electron:dist-win": "pnpm build && pnpm electron:build-ts && electron-builder --config electron-builder.yml --win"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.33",
    "@ai-sdk/google": "^3.0.18",
    "@ai-sdk/openai": "^3.0.23",
    "@resvg/resvg-wasm": "^2.6.2",
    "ai": "^6.0.63",
    "sql.js": "^1.12.0",
    "htmlparser2": "^10.1.0",
    "js-yaml": "^4.1.1",
    "liquidjs": "^10.24.0",
    "mupdf": "^1.27.0",
    "next": "16.1.6",
    "pngjs": "^7.0.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-image-crop": "^11.0.10",
    "rxjs": "^7.8.2",
"zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20",
    "@types/sql.js": "^1.4.9",
    "@types/pngjs": "^6.0.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
"@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.18",
    "electron": "^34.0.0",
    "electron-builder": "^26.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "electron"
    ]
  }
}